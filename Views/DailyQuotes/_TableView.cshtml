@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<DailyPerformanceReportAuto.Models.DailyQuote> _ConfTQ = (List<DailyPerformanceReportAuto.Models.DailyQuote>)ViewBag.ConfusedTopQ;
    List<string> _CTQ = (List<string>)ViewBag._CTQ;
    List<string> _PTQHI = (List<string>)ViewBag._PTQHI;
    List<string> _CTopQHI = (List<string>)ViewBag._CTopQHI;
    List<string> _CTopQPHI = (List<string>)ViewBag._CTopQPHI;
    List<string> _PTQQYH = (List<string>)ViewBag._PTQQYH;
