<div>
    <h2>@_ConfTQ[0].QuoteSource</h2>

    <h3>Confused</h3>

    <table style="border:5px solid red; width: 100%">
        <tr>
            <th>1</th>
            <th>2</th>
            <td>
                <table id="innerTable">
                    <tr>
                        <td>Saturday</td>
                        <td>Sunday</td>
                        <td>Monday</td>
                        <td>Tuesday</td>
                        <td>Wednesday</td>
                        <td>Thursday</td>
                        <td>Friday</td>
                        <td class="lrgcell">Status</td>
                    </tr>
                </table>
            </td>
        </tr>

        <tr>
            <th>Total Quotes</th>
            <th>
                <table id="innerTable">
                    <tr><td>@ViewBag._CTQDatestring[0].ToString()</td></tr>
                    <tr><td>@ViewBag._CTQDatestring[1].ToString()</td></tr>
                </table>
            </th>

            <td>
                <table id="innerTable">
                    <tr>
                        @{
                            var c = 7;

                            for (int d = 0; d < _CTQ.Count; d++)
                            {
                                <td>@_CTQ[c].ToString()</td>
                                c = c - 1;

                                if (c == -1)
                                {
                                    @Html.Raw("</tr><tr>")
                                    c = 14;
                                }
                            }
                        }
                    </tr>
                </table>
            </td>
        </tr>


